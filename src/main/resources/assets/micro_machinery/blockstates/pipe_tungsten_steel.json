{
	"multipart": [
		{
			"apply": {
				"model": "micro_machinery:block/pipe/pipe_tungsten_steel"
			}
		},
		{
			"when": {
				"north_connect": "pipe"
			},
			"apply": {
				"model": "micro_machinery:block/pipe/pipe_arm3"
			}
		},
		{
			"when": {
				"east_connect": "pipe"
			},
			"apply": {
				"model": "micro_machinery:block/pipe/pipe_arm3",
				"y": 90
			}
		},
		{
			"when": {
				"south_connect": "pipe"
			},
			"apply": {
				"model": "micro_machinery:block/pipe/pipe_arm3",
				"y": 180
			}
		},
		{
			"when": {
				"west_connect": "pipe"
			},
			"apply": { "model": "micro_machinery:block/pipe/pipe_arm3", "y": 270}
		},
		{   "when": { "up_connect": "pipe" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_arm3", "x": 270}
		},
		{   "when": { "down_connect": "pipe" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_arm3", "x": 90}
		},
		{   "when": { "north_connect": "connect" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_connect3"}
		},
		{   "when": { "east_connect": "connect" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_connect3", "y": 90}
		},
		{   "when": { "south_connect": "connect" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_connect3", "y": 180}
		},
		{   "when": { "west_connect": "connect" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_connect3", "y": 270}
		},
		{   "when": { "up_connect": "connect" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_connect3", "x": 270}
		},
		{   "when": { "down_connect": "connect" },
			"apply": { "model": "micro_machinery:block/pipe/pipe_connect3", "x": 90}
		}
	]
}